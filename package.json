    "babel-preset-env": "^1.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.8.2",
    "flow-bin": "^0.42.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4"
