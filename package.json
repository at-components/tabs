{
  "version": "0.2.3",
  "name": "@components/tabs",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && babel src -d lib",
    "clean": "rm -rf lib && mkdir lib",
    "flow": "flow",
    "lint": "eslint src/**/*.js",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "author": "case <case@casesandberg.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@case/eslint-config",
    "env": {
      "jest": true
    },
    "rules": {
      "react/sort-comp": [
